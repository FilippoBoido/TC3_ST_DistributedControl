  <POU Name="MAIN" Id="{a428ac16-d16d-414b-aa2c-821dfdc90fce}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	fbPayload_1 : Payload(pSubclassPointer := ADR(fbPayload_1),nObjectSize:=SIZEOF(fbPayload_1));
	fbPayload_2 : Payload(pSubclassPointer := ADR(fbPayload_2),nObjectSize:=SIZEOF(fbPayload_2));
	
	bStart,bStart2,bStart3 : BOOL;
	Size1,Size2,Size3,Size4 : UINT;
	
	pBuffer : POINTER TO BYTE;
	aMarshalBuffer : ARRAY[0..1999] OF BYTE;
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[fbPayload_1();
fbPayload_2();
fbPayload_1.timer.PT := T#45S;
fbPayload_1.timer.IN := TRUE;

IF 	bStart
	THEN
		
	pBuffer := fbPayload_2.Deserialize(fbPayload_1.Serialize());
	__DELETE(pBuffer);
	bStart := FALSE;
	
END_IF

IF	bStart2
	THEN
	
	pBuffer := fbPayload_1.Serialize();
	MEMCPY(ADR(aMarshalBuffer),pBuffer,fbPayload_1.nObjectSize);
	__DELETE(pBuffer);
	bStart2 := FALSE;
	
END_IF

IF	bStart3
	THEN
	
	fbPayload_2.Deserialize(ADR(aMarshalBuffer));
	//MEMCPY(ADR(fbPayload_2),ADR(aMarshalBuffer),fbPayload_2.nObjectSize);
	bStart3 := FALSE;
	
END_IF]]></ST>
    </Implementation>
